generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int       @id @unique @default(autoincrement())
  name          String
  age           Int
  gender        String    @db.VarChar(10)
  product       product[]
}

model product {
  id     Int  @unique @default(autoincrement())
  User   User @relation(fields: [userId], references: [id])
  userId Int
  name String 
}
